# Example .env for BlazeJob Web3/FinTech/Solana/Email/Cosmos
PRIVATE_KEY=0xYOUR_PRIVATE_KEY
RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY

# Solana
SOLANA_SECRET_KEY=your_solana_secret_key_base58
